package uk.ac.gla.dcs.tp3.w.parser;

import java.io.File;
import java.io.FileNotFoundException;
import java.io.IOException;
import java.util.Scanner;

/**
 * @author James & Kris
 *
 */
public class Parser {
	public static void main(String[] args) {
		Scanner fileScanner, lineScanner;
		File source = new File("/users/level3/1007175s/Sports-Elimination-TP3/src/uk/ac/gla/dcs/tp3/w/parser/src/baseballSource.txt");
		try {
			fileScanner = new Scanner(source);
			while(fileScanner.hasNextLine()){
				String thisLine = fileScanner.nextLine();
				lineScanner = new Scanner(thisLine);
				lineScanner.next();						//get date
				lineScanner
			}
		} catch (FileNotFoundException e){
			System.out.println("Wrong filename");
		}
	}

}
